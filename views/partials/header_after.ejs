<script>
    function myFunction() {
        var x = document.getElementById("myLinks");
        if (x.style.display === "block") {
            x.style.display = "none";
        } else {
            x.style.display = "block";
        }
    }

</script>

<body>
    <div class="grid-header">
        <div class="div1">
            <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                <i class="fa fa-bars"></i>
            </a>
        </div>
        

        <div id="myLinks" class="dropdown-content">
            <a href="/home">Home</a>
            <a href="/profile">Profile</a>
            <a href="/matching">Matching</a>
            <a href="/chat">Chatting</a>
            <a href="/signout">Log out</a>
        </div>
        <div class="div2">
            <img src="/img/Linkup_logo.png" alt="LinkUp Logo" class="logo">
            <span class="header-text">LinkUp</span>
        </div>
        <div>
            <a href="/signout">
                <img src="/img/logout_button.png" alt="Logout button" class="logout_button">
            </a>
        </div>
    </div>