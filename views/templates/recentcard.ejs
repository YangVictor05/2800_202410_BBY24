    <div id="matched-template">
        <div class="recent-image"> 
            <img src="<%= matchuser.profilePicture %>" alt=""> 
        </div>
        <div class="user-details">
            <h4 id="name"><%= matchuser.name %></h4>
            <p>Biography: <%= matchuser.biography %></p> 
            <form action="/chattingnow" method="post" style="justify-content: right; margin-bottom: 0.5em; margin-right: 0.5em;">
                <input type="hidden" name="matchEmail" value="<%= matchuser.email %>">
            <button class="mybutton" style="font-size: 0.6em;">Chat with user</button>
            </form>
        </div>
    </div>
