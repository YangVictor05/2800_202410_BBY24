   <div class="eventcardbody">
    <a href="/view_event/<%= events._id %>" style="text-decoration: none" id="event">
    </a>
      <img src="<%= events.eventPicture%>" alt="..." class="event_picture" onerror="this.onerror=null; this.src='/img/defaulteventpic.png'">
      <div class="event_details">
        <button class="editbtn">
          <span class="material-symbols-outlined" id="editicon">
            edit_square
          </span>
        </button>
        <h3 class="event_title"><%= events.eventName %></h5>
          <h5 class="event_access">Event access</h5>
          <div class="date">
            <span class="material-symbols-outlined" id="calendar">
              event
            </span>
            <h4 class="event_date"><%= events.eventDate %></h4>
          </div>
          <div class="time">
            <span class="material-symbols-outlined" id="clock">
              schedule
            </span>
            <h4 class="event_time"><%= events.eventTime %></h4>
          </div>
          <div class="location">
            <span class="material-symbols-outlined" id="locationpin">
              location_on
            </span>
            <h4 class="event_location"><%= events.evenLocation %></h4>
          </div>
          <h4 class="event_description"><%= events.description %> </h4>
      </div>
    </div>
