<div id="event-container">
    <% let i=0; %>
        <% for(i=0; i < event.length; i++) { %>
        <%- include('../templates/eventcard', {events: event[i]})  %> 
    <% } %> 
</div>
<h2>Saved users</h2>
<div id="people">
    <% if (users.length > 0) { %>
        <% let j=0; %>
        <% for(j=0; j < Math.floor(3, users.length); j++) { %>
        <%- include('../templates/recentcard', {matchuser: users[j]})  %> 
         <% } %> 
      <% } else { %>
      <% } %>
</div>

